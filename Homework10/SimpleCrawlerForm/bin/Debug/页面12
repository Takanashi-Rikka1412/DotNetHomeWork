            <script>
		function moreEvaluation(courseId){
				jQuery(".viewmore").hide();
		 		jQuery("#pageNum").val(parseInt(jQuery("#pageNum").val())+1);
				var page = jQuery("#pageNum").val();
				jQuery.ajax({ //一个Ajax过程 
					type: "get", //以get方式与后台沟通 
					url : "/CourseController/courseEvaluation", 
					dataType:'html',//返回的值
					data: {courseId:courseid,edit:editvalue,page:page,hiddenMyEvaluation:1}, 
					success: function(data){//如果调用成功 
					    jQuery('#courseEvaluation').append(data);
					} 
				}); 
		}
        jQuery(".w_menu_item").on("click",function(){
            jQuery(this).addClass("wcurr").siblings().removeClass("wcurr");
            var  id = jQuery(this).attr("id");
            var  parentId = jQuery(this).attr("parentId");
            var array = document.getElementsByName("ariticleParentId_"+parentId);
            if(array != null){
                for(var i = 0 ; i<array.length;i++){
                    array[i].css('display','none');
                }
            }
            jQuery("#ariticlecontent_"+id).css('display','block');
        })
        if(207646926== '200119420'){
            jQuery("#courseNavList li").each(function(){
                if(jQuery(this).html().indexOf('课程评价') > 0 ){
                    jQuery(this).css('display','none');
                }
            });
        }
	</script>

