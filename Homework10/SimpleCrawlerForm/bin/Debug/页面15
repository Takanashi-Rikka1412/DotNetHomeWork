            userAgent = userAgent.toLocaleLowerCase();
            if (userAgent.indexOf("ios") != -1 || userAgent.indexOf("android") > -1) {
                new_element = document.createElement('link');
                new_element.setAttribute('rel', 'stylesheet');
                new_element.setAttribute('type', 'text/css');
                new_element.setAttribute('href', '/css/phone/750.css');
                document.body.appendChild(new_element);
            }

            jQuery(".w_menu_item").on("click",function(){
                jQuery(this).addClass("wcurr").siblings().removeClass("wcurr");
                var  id = jQuery(this).attr("id");
                var  parentId = jQuery(this).attr("parentId");
                var array = document.getElementsByName("ariticleParentId_"+parentId);
                if(array != null){
                    for(var i = 0 ; i<array.length;i++){
                        array[i].css('display','none');
                    }
                }
                jQuery("#ariticlecontent_"+id).css('display','block');
            })

            var courseId=214450420;
            var arrMetroH = [], maxHeight = 0;
            $$(".metroListBox").each(function(ele,i){
            arrMetroH.push(ele.h())
            maxHeight = Math.max(arrMetroH[i], maxHeight);
            });
			
			if(2==22)
			 	var arrMetroColors = ["#59aa9e","#8d95b5","#9ab474","#49b4bb","#cbb982","#689dc1","#777777","#3f8408"];
			
			else if(2==48)
				var arrMetroColors = ["#f15f67","#83c54f","#3f8408","#e8891a","#9c68b7","#3cb9e7","#4c84b8","#777777"];
            else 
				var arrMetroColors = ["#e8891a","#f15f67","#3cb9e7","#4c84b8","#83c54f","#9c68b7","#777777","#3f8408"];
			
			
            $$(".metroListBox").each(function(ele,i){
            var height = maxHeight - arrMetroH[i];
            if(height > 0){
            var fillingBlock = new Element('div',{'class':'metroListSingle'});
            var Elements = ele.getChildren();
            if(ele.getChildren().length > 0){
            fillingBlock.css('height',height-2).inject(ele.getChildren().getLast(), 'after');
            }else{
            fillingBlock.css('height',height-4).inject(ele, 'bottom');
            }
            }
            ele.getChildren(".metroListSingle").each(function(ele,j){
            ele.css('background',arrMetroColors[(j+3*i) % 8]);
            });
            });
        </script>
