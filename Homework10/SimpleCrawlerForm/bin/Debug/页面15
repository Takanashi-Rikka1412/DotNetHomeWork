jQuery(function(){
    try{
        //获取登录状态
        var login = true;
        var fid = jQuery.cookie("fid");
        var loginString = "_uid";
                		var loginStatus=jQuery.cookie(loginString);
						//alert(loginStatus);
                		if(loginStatus==undefined) login=false;
        	          //  alert(login);
						
                		//请求访问数
                		jQuery.ajax({ //一个Ajax过程 
                			type: "get", //以get方式与后台沟通 
                			url : "/course/viewTimesTag", 
                			dataType:'html',//返回的值
                			data: {courseId:courseid,loginStatus:login},
                			success: function(data){//如果调用成功 
                			    jQuery('#viewTimesTag').append(data);
                			} 
                		}); 
    }catch(e){
	  alert(e.message);
	 }
});
