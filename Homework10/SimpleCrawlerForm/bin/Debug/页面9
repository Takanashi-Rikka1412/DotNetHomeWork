<script type="text/javascript">
 window.onload=function(){
 	var knowledgeId = $("#knowledgeId").val();
    var indexnum=1;
	$(".module").css('display','none');
	$(".module"+indexnum).css('display','block');
    var ali =$(".navBg td");
	
	ali.find("a").removeClass('click');
	$(".navBg td").eq(indexnum-1).find("a").addClass("click")
    var modulesize=Math.ceil(ali.length);
		ali.click(function(){
            if (((ali.index(this)) == 1) &&  (1 == 1)) {
                read()
            }
            else {
                var indexnum = ali.index(this) + 1;
                var module = "module" + indexnum;
                $(".module").css('display', 'none');
                var date = new Date().getTime();
                location.href = "?edit=false&knowledgeId=" + knowledgeId + "&module=" + indexnum + "&v=" + date + "#content";
            }
		});
		



