$(function(){
	var size = $(".navBg tbody tr td").size();
    var otherSize = $(".znavMore-site ul li").size();
    var navBg =  $(".nav");

    if(otherSize > 0) {
        $(".navBg tbody td a").css("width",(900-2*size)/size);
        navBg.css("width", '900');
    }else {
        $(".navBg tbody td a").css("width",(1000-2*size)/size);
        navBg.css("width", '1000');
    }


	if ((207161794 == 200018483 || 207161794 == 200018489) ) {
		$(".navBg tbody td a").eq(0).css('width','47px');
		$(".navBg tbody td a").eq(1).css('width','125px');
		$(".navBg tbody td a").eq(2).css('width','125px');
	}

	$('.znavMore-js').mouseover(function () {
		$(this).find('.znavMore-site').slideDown(100)
	}).mouseleave(function () {
		$(this).find('.znavMore-site').slideUp(100)
	})
});
</script>
