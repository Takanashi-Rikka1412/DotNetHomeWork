<script type="text/javascript">
$(function(){
	$(".courselist").click(function(){
		if($(this).next(".courselistCon").is(":hidden")){
			$(this).next(".courselistCon").show();
		} else {
			$(this).next(".courselistCon").hide()
		}
	})
	
	$(".secondCap p").click(function(){
		$(".secondCap p").removeClass("click")
		$(this).addClass("click")
	})
	
	
	$('.courselistCon ul li').click(function(){
		$('.courselistCon ul li').removeClass("click");
		$(this).addClass("click");
	});
})
</script>

