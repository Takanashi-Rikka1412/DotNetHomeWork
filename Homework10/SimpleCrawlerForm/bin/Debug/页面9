var switchModule = function (index, edit) {
    var knowledgeId = $("#knowledgeId").val();
    var date = new Date().getTime();
    location.href = "?edit=" + edit + "&knowledgeId=" + knowledgeId + "&module=" + index + "&v=" + date + "#content";
}
