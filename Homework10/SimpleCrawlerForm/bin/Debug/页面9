<script type="text/javascript">

function read(){
var tip="该章节不能试读，若要继续学习，请登录或报名！";
var status=jQuery("#clazzTag a").text();
if(status.indexOf("进入课程")!=-1){
tip="该章节不能试读，若要继续学习，请点击\"进入课程\"进行学习。";
}
alert(tip);
}

function openNext(privately){
	if(privately == 1){
		alert("该课程不可试读");
	} else if (privately == 3) {
		alert("该课程只可试读第一章");
	} else if (privately == 2) {
		alert("该课程只可试读前三章");
	}
}

</script>
