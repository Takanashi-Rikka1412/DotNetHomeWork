jQuery(function(){
	    try{
            //获取登录状态
            var login = true;
            var fid = jQuery.cookie("fid");
            var loginString = "_uid";
            var loginStatus = jQuery.cookie(loginString);
            if (loginStatus == undefined) login = false;
            //请求访问数
			jQuery.ajax({ //一个Ajax过程 
				type: "get", //以get方式与后台沟通 
				url : "/course/viewTimesTag", 
				dataType:'html',//返回的值
				data: {courseId:courseid,loginStatus:login}, 
				success: function(data){//如果调用成功 
				    jQuery('#viewTimesTag').append(data);
				} 
			});
	    }catch(e){alert(e.message)}
});
jQuery(function(){
      function subStr(str,n){
		 var filename=str.replace(/.*(\/|\\)/, "");
		 var fileExt=(/[.]/.exec(filename)) ? /[^.]+$/.exec(filename.toLowerCase()) : '';
		 var strReg=/[^\x00-\xff]/g;  
         var _str=filename.replace(strReg,"**");  
	     var _len=_str.length;
		 if(_len>n){  
		 var _newLen=Math.floor(n/2);  
		 var _strLen=str.length;  
		 for(var i=_newLen;i<_strLen;i++){  
			 var _newStr=str.substr(0,i).replace(strReg,"**");  
			 if(_newStr.length>=n){  
				 return str.substr(0,i)+"...."+fileExt;  
				 break;  
			}  
		 }  
	   }else{  
		 return str;  
	   }  
   	}
	  jQuery(".origin").each(function(){
	  	var content=jQuery(this).text();
		jQuery(this).text(subStr(content,20));
	  })
});	
	
	var $ =mootools ;
</script>
