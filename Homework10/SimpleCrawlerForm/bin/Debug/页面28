//��ʼ��ר����Ŀ������Ŀ����ʾ����
function initChildNewsList(){
	var arChildDiv		= document.all("NewsList");
	if(arChildDiv == null){
		alert("��ʼ��ʧ�ܣ�û�ж���NewsListԪ�أ�");
		return;
	}
	//��ȡ���������б���Ŀ
	var nCount			= 1;
	if(arChildDiv.length)nCount = arChildDiv.length;

	var arChildChnlName = document.all("ChildChnlName");
	if(arChildChnlName == null){
		alert("��ʼ��ʧ�ܣ�û���κ���Ƶ����");
		return;
	}
	//��ȡʵ�ʵ������б���Ŀ
	var nRelCount			= 1;
	if(arChildChnlName.length)nRelCount = arChildChnlName.length;

	//��û�����ŵ��б�����
	for(var i=nRelCount; i<nCount; i++)
	{
		arChildDiv[i].style.display = "none";
	}
}

initChildNewsList();
</script>
