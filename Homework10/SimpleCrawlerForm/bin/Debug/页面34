<script>
  $(document).ready(function() {
	$('.pagination').jqPagination({
		link_string	: '/?page={page_number}',
		max_page	: 5,
		paged		: function(page) {
		     
		             ajaxImg(page);
		 }
	});
	ajaxImg(1);
});
function ajaxImg(page){
			$.ajax({
 
                 cache: false,
 
                 type: "POST",
 
                 url:"/cae/sites/main/video_servlet.jsp", //把表单数据发送到video_servlet
 
                 data:{pagenumber:page,columnid:209}, //要发送的是ajaxFrm表单中的数据
 
                 async: false,
 
                 error: function(request) {
 
                   alert("发送请求失败！");
 
              },
 
              success: function(dbset) {
			  if(dbset.length>0){
			      if(dbset.length==8){
				  //没有数据
			    $("#divpicture").append("<ul id='uu'></ul>");
				$("#uu").append("<li><p><img  width='190px' height='150px' src='/cae/sites/main/images/nullvideo.png' /></p><h4>暂无相关数据</h4></li>"); 
					}else{
					    var star=dbset.indexOf("[");
				  var end=dbset.lastIndexOf("]");
				  
				 dbset = dbset.substr(star+1,end-1);
				
                   var ss = dbset.split("*^*,");
				   if(ss.length>0){
				       $("#divpicture").empty();
				       $("#divpicture").append("<ul id='uu'></ul>");
				   }
				    var tid;
				   for(var i=0;i<ss.length;i++){
				         tid=ss[i+2];
				      if(i+2==ss.length-1){
					      tid=ss[i+2].substring(0,ss[i+2].length-1);
					  }
				   $("#uu").append("<li><p><a href='http://www.cae.cn/cae/sites/main/video_play.jsp?tid="+$.trim(tid)+"'><img  width='190px' height='150px' src='"+ss[i]+"' /></a></p><h4><a href='http://www.cae.cn/cae/sites/main/video_play.jsp?tid="+$.trim(tid)+"'>"+ss[i+1]+"</a></h4></li>");
					i++;i++;
			      }
					}
			      
			  }
                }
 
             });
	}
</script>
