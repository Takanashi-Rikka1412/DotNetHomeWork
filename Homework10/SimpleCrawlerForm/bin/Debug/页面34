function addFavCommon() {
 try {
    window.external.addFavorite('http://www.cae.cn', '中国工程院');//IE
 } catch(e) {
  try {
      window.sidebar.addPanel('中国工程院', 'http://www.cae.cn', '中国工程院');//firefox
  } catch(e) { 
      alert("加入收藏失败，请使用Ctrl+D进行添加");
  }
 }
}
