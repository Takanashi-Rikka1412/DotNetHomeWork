               //初始化举报按钮
        jQuery(document).ready(function () {
            var is_login = jQuery("#is_login").val();
            var is_teacher = jQuery("#is_teacher").val();
            if (is_login == 'false') {
                //未登录
                jQuery("#toReport").attr('onclick', '_msgtip()');
            } else if (typeof(is_teacher) != "undefined" && is_teacher == 'true') {
                //教师身份
                jQuery("#_report").remove();
            }
        })

	   function showSemesterList(){
		   var display = jQuery("#options").css("display");
	  	   if(display == "none"){
	  	  		jQuery("#options").show();
	  	   }else{
	  	   		jQuery("#options").hide();
	  	   }
	      
	    }

