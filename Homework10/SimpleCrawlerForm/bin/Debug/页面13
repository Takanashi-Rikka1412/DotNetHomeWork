<script>
  	jQuery(".chapterText").each(function(){
    	jQuery(this).html(jQuery(this).html().replace(/&nbsp;/ig," "));
  	});
	function loadVideo() {
		var oMoocPlayer = jQuery('#moocplayer');
		 var sVideoData =  oMoocPlayer ? oMoocPlayer.attr('data') : null;
		 if (sVideoData) {
		        sVideoData=sVideoData.replace(/\\~/g,"'");// 单引号替换过来
			    var oVideoData = JSON.parse(sVideoData);
        		var videoUrl=oVideoData.http;
        		var priviewUrl=oVideoData.screenshot;
        		var hd_videoUrl=oVideoData.httphd;
        		if(document.domain=='haust.edu.cn'){
        			videoUrl=videoUrl.replace("chaoxing.com",document.domain);
        			priviewUrl=priviewUrl.replace("chaoxing.com",document.domain);
        			hd_videoUrl=hd_videoUrl.replace("chaoxing.com",document.domain);
        		}
				var player = new SimplePlayer({
				render: 'moocplayer',
				videoUrl: videoUrl,
				priviewUrl:priviewUrl,
				hd_videoUrl:hd_videoUrl
				
			});
			
		}
	}
	
loadVideo();
    var play=document.getElementById("zvideo_play");
    if(play){
    play.onclick=function(){
        var zplay=document.getElementById("zvideo");
        zplay.style.display="block";
        this.style.display="none";
    	
    }
 }
var courseStar=document.getElementById("courseStarTag");
var oclazzTag=document.getElementById("clazzTag");
var editvalue,courseid,isLogin,templatevalue;
if(oclazzTag){  
   editvalue=oclazzTag.getAttribute("data");
   isLogin=oclazzTag.getAttribute("flag");
   templatevalue=oclazzTag.getAttribute("template");
}
if(courseStar){
	courseid=courseStar.getAttribute("data");
}
	var flag=false;
	if(isLogin)
	flag=true;
	
	function checklogin(){
		if(!flag){
			alert("请先登录")
		}else{
			window.location.href="/coursestar?courseId=214685837&edit="+editvalue;
		}
	} 

jQuery.ajax({ 
			type: "get", 
			url : "/course/clazzTag", 
			dataType:'html',
			data: {courseId:courseid,edit:editvalue,template:templatevalue}, 
			success: function(data){//如果调用成功 
			   jQuery('#clazzTag').prepend(data);
			} 
		});
jQuery.ajax({ 
			type: "get", 
			url : "/CourseController/getCourseStarTag", 
			dataType:'html',
			data: {courseId:courseid,edit:editvalue}, 
			success: function(data){
				//alert("getCourseStarTag");
				jQuery('#courseStarTag').prepend(data);
    		    //document.getElementById("courseStarTag").innerHTML=data;
			} 
		}); 
jQuery.ajax({ //一个Ajax过程 
			type: "get", //以get方式与后台沟通 
			url : "/CourseController/courseEvaluation", 
			dataType:'html',//返回的值
			data: {courseId:courseid,edit:editvalue}, 
			success: function(data){//如果调用成功 
			    jQuery('#courseEvaluation').append(data);
			} 
		}); 
	
var oPriceCourse=document.getElementById("priceCourse");
if(oPriceCourse){
jQuery.ajax({ //一个Ajax过程 
	type: "get", //以get方式与后台沟通 
	url : "/course/priceTag", 
	dataType:'html',//返回的值
	data: {courseId:courseid}, 
	success: function(data){//如果调用成功 
	    jQuery('#priceCourse').append(data);
	} 
});
