                function supportH5Video() {

                    var videoMp4Support = function() {
                        var elem = document.createElement('video');
                        if ( typeof elem.canPlayType == 'function') {
                            var playable = elem.canPlayType('video/mp4;codecs="avc1.42E01E,mp4a.40.2"');
