<script type="text/javascript">
		function isPrivately(isPrivately){
		if(isPrivately == 1){
			alert('关闭试读');
		} else if(isPrivately == 2){
			alert('试读前三章');
		} else if(isPrivately == 3){
			alert('试读第一章');
		}
	}
