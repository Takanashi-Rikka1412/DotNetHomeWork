   editvalue=oclazzTag.getAttribute("data");
   isLogin=oclazzTag.getAttribute("flag");
   templatevalue=oclazzTag.getAttribute("template");
}
if(courseStar){
	courseid=courseStar.getAttribute("data");
}
	var flag=false;
	if(isLogin)
	flag=true;
	
	function checklogin(){
		if(!flag){
			alert("请先登录")
		}else{
			window.location.href="/coursestar?courseId=216567111&edit="+editvalue;
		}
	} 

jQuery.ajax({ 
			type: "get", 
			url : "/course/clazzTag", 
			dataType:'html',
