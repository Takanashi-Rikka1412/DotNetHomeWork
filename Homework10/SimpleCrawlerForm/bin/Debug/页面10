	jQuery(function(){
		jQuery(".M1C2").hover(function(){
			jQuery(this).children().eq(2).show();
			},function(){
				jQuery(this).children().eq(2).hide();
				})
		})
	jQuery(function(){
		jQuery(".Djckxq").click(function(){
			jQuery(this).hide();
			jQuery(this).next().show();
			jQuery(this).parent().prev().show();
			})
		jQuery(".Djsq").click(function(){
			jQuery(this).hide();
			jQuery(this).prev().show();
			jQuery(this).parent().prev().hide();
			})
	})
	jQuery(function(){
		jQuery(".MRc1bt a").click(function(){
			if(jQuery(this).parent().next().is(":hidden")){
				jQuery(".MRc1ul").hide();
				jQuery(this).parent().next().show();
				} else {
				jQuery(this).parent().next().hide();
					}

	 })
		
   })
</script>
