<script>
    uParse(".ans-cc");
    audiojs.events.ready(function() {
    var as = audiojs.createAll();
    });
    //课程介绍展开收起
    var courseInfoFold = $("courseInfoFold");
    if ($isEle(courseInfoFold)){
    var courseInfoBox = $("courseInfoBox"), courseInfoTxt = $("courseInfoTxt"), courseTxtDot = $("courseTxtDot");
    var height = courseInfoFold.attr("data");
    var classStr = "";
    switch(height){
    case "380":
    classStr = "h380";
    break;
    case "330":
    classStr = "h330";
    break;
    default:
    classStr = "h150";
    height = 151;
    break;
    }	
    if(courseInfoTxt.getSize().y > height){
    courseInfoBox.addClass(classStr);
    courseInfoTxt.addClass(classStr);
    courseTxtDot.removeClass("abs_out");
    courseInfoFold.removeClass("abs_out");
    }
    courseInfoFold.addEvent("click", function(){
    if (this.txt() === "[展开]") {
    courseInfoBox.removeClass(classStr);
    courseInfoTxt.removeClass(classStr);
    courseTxtDot.hide();
    this.txt("[收起]")
    } else {
    courseInfoBox.addClass(classStr);
    courseInfoTxt.addClass(classStr);
    courseTxtDot.show();
    this.txt("[展开]")
    }
    })
    }
    var tchDescBox = $('tchDescBox');
    $powerFloat($$('.course_tch_item'), {
    position: "4-1",
    offsets: {x:0, y:0},
    target: tchDescBox,
    showCall: function(){
    var tchDesc = $('tchDescContent'), tchDescInnerBox = $('tchDescInnerBox');
    tchDesc.html(this.attr('data'));
    if(tchDesc.h() < 200) {
        tchDescInnerBox.h(tchDesc.h() + 40);
        }else{
        tchDescInnerBox.h(240);
        }
        },
        hideCall: function(){
        }
        });
        var initStarNum = $$('.i_50').length, initGrayStarArr = $$('.i_49');
        $$('.jsForStar').each(function(item, index){
        item.store('initGray', item.getElement('i').hasClass('i_49'));
        item.addEvents({
        'mouseenter': function(){
	           if(item.getElement('i').hasClass('i_49')){
		           for(var i = 0; i < index+1; i++){
			            if($$('.jsForStar')[i].getElement('i').hasClass('i_49')){
			               $$('.jsForStar')[i].getElement('i').swapClass('i_49', 'i_50');
			            }
		            }
	            }
            },
            'mouseleave': function(){
                if(item.retrieve('initGray') && item.getElement('i').hasClass('i_50')){
	            	for(var i = 0; i < initGrayStarArr.length; i++){
		        	        if(initGrayStarArr[i].hasClass('i_50')){
		                	initGrayStarArr[i].swapClass('i_50', 'i_49');
		                }
	                }
                }
             },
                'click': function(){
                    for(var i = index+1; i < $$('.jsForStar').length; i++){//当前星星的后面均变灰
	                    if($$('.jsForStar')[i].getElement('i').hasClass('i_50')){	
		                    $$('.jsForStar')[i].getElement('i').swapClass('i_50', 'i_49');
		                    $$('.jsForStar')[i].store('initGray', true);
	                    }
                    }
                    if(index === 0 && !item.retrieve('initGray')){
                    	item.getElement('i').swapClass('i_50', 'i_49');
                    }
                    new AjaxPost(item, {
	                    url: '/CourseController/setCourseStar',
	                    data: {courseId: $('courseStars').attr('data'), starNumber: $$('.i_50').length},
	                    callback: function(json){
		                    	if(json.succ){
			                    	$('personCount').txt(new Number($('personCount').txt()) + 1);
			                   		 var html = '';
			                     	for(var i = 0; i < new Number(json.html); i++){
			                        	html += '<i class="i i_50"></i>';
			                        }
			                        $('courseStars').html(html);
			                        $('courseStars').attr('flag', 'true');
			                        $testRemind($('courseStars'), '评价成功！', 1);
			                        setTimeout('$testRemind.hide()',2000);
		                        }else{
			                        $('courseStars').attr('flag', 'false');
			                        $testRemind($('courseStars'), '评价失败！', 1);
			                        setTimeout('$testRemind.hide()',2000);
		                        }
	                        }
                      }).send();
                  }
                        })
                        })
</script>
<script type="text/javascript">
   var pingfen = function(btn){
    	if($isEle(btn)){
    		btn.addEvent('click', function(){
				//打开窗口
			     document.getElementById("zopacity").style.display="block";
			     document.getElementById("pingfenContent").style.display="block";
				 if(curPersonStar > 0){
					document.getElementById("id_zs_item").style.display="none";
				 }
				 //窗口定位
				 var heit = window.screen.availHeight,
                    wid = window.screen.availWidth,
                    alertwid = document.getElementById("pingfenContent").css("width").replace("px",""),
                    left = (wid-alertwid)/2,
					top = heit/8; 
                    document.getElementById("pingfenContent").css({position: "absolute",top: top, left: left,zIndex:9999}); 
					
					//加载评价记录列表
					new AjaxPost(btn, {
        				url : '/CourseController/CourseStarContextReplyList',
        				data : {
        					courseId: $('courseStars').attr('data')
        				},
        				callback : function(json) {
        					if (json.succ) {
								$('replyList').html("");
								for(var i=0;i<json.contextReplyList.length;i++){
									var _width = json.contextReplyList[i].starNumber*18+(json.contextReplyList[i].starNumber-1)*6;
									var html = ""+
    							    "<div class=\"zs_item\">"+
                                    	"<div class=\"zs_i_gray\">"+
                                        	"<div class=\"zs_i_re\">"+
                                            	"<div class=\"zs_i_abs\">"+
                                                	"<p class=\"zs_i_portrait\"><img src=\""+json.contextReplyList[i].userImg+"\" width=\"50\" height=\"50\" /></p>"+
                                                    "<ul class=\"zs_i_infor\">"+
                                                    	"<li class=\"zs_i_top\"><span>"+json.contextReplyList[i].username+"</span><span class=\"gray\">发表于</span><span class=\"gray\">"+json.contextReplyList[i].createTimes+"</span></li>"+
                                                        "<li class=\"zs_padding5\">"+
                                                        	"<dl class=\"zs_star_gray\">"+
                                                                "<dd class=\"zs_star_color\" style=\"width:"+_width+"px;\"></dd>"+
                                                            "</dl>"+
                                                		"</li>"+
                                                    "</ul>"+
                                                "</div>"+
                                            "</div>"+
                                            "<div>"+json.contextReplyList[i].context+"</div>"+
                                        "</div>"+
                                    "</div>";
									
    								$('replyList').html($('replyList').html()+html);
									
								}
							    if(json.contextReplyList.length<=0){
									$('replyList').html('暂无评论！');
								}
        					}
        				}
        			}).send();
    		});
    	}
    }
var close_pingfen = function(btn){
    	if($isEle(btn)){
    		btn.addEvent('click', function(){
				 document.getElementById("zopacity").style.display="none";
				 document.getElementById("pingfenContent").style.display="none";
    		});
    	}
    }
close_pingfen($('_zs_close'))

var commitPingfen = function(btn){
	if($isEle(btn)){
		btn.addEvent('click', function(){
		var _context = $('zs_i_textarea').val(); 
    		if(_context.length>0){
    		   _context = _context.replace(/^\s+|\s+$/g, ""); 
    		   if(_context.length>0&&_context.length<150){
    		       new AjaxPost(btn, {
        				url : '/CourseController/setCourseStarContext',
        				data : {
        					context : _context,
        					courseId: $('courseStars').attr('data')
        				},
        				callback : function(json) {
        					if (json.succ) {
								$('zs_i_textarea').val(""); 
								document.getElementById("id_zs_item").style.display="none";
								var beforehtml = $('replyList').html();
								if(beforehtml=="暂无评论！"){
									$('replyList').html("")
								}
								
							 	var _width = curPersonStar*18+(curPersonStar-1)*6;
								var html = ""+
							    "<div class=\"zs_item\">"+
                                	"<div class=\"zs_i_gray\">"+
                                    	"<div class=\"zs_i_re\">"+
                                        	"<div class=\"zs_i_abs\">"+
                                            	"<p class=\"zs_i_portrait\"><img src=\""+json.curImg+"\" width=\"50\" height=\"50\" /></p>"+
                                                "<ul class=\"zs_i_infor\">"+
                                                	"<li class=\"zs_i_top\"><span>"+json.curUserName+"</span><span class=\"gray\">发表于</span><span class=\"gray\">"+json.publicTime+"</span></li>"+
                                                    "<li class=\"zs_padding5\">"+
                                                    	"<dl class=\"zs_star_gray\">"+
                                                            "<dd class=\"zs_star_color\" style=\"width:"+_width+"px;\"></dd>"+
                                                        "</dl>"+
                                            		"</li>"+
                                                "</ul>"+
                                            "</div>"+
                                        "</div>"+
                                        "<div>"+_context+"</div>"+
                                    "</div>"+
                                "</div>";
								$('replyList').html(html+$('replyList').html());
        					}else{
        					    alert(json.msg);
        					    return;
        					}
        				}
        			}).send();
    		    }else{
				   alert("0<字数<150");
				}
    		}
		});
	}
}
commitPingfen($('commitPingfen'))

function read(){
var tip="该章节不能试读，若要继续学习，请登录或报名！";
var status=jQuery("#clazzTag a").text();
if(status.indexOf("进入课程")!=-1){
tip="该章节不能试读，若要继续学习，请点击\"进入课程\"进行学习。";
}
alert(tip);
} 
