<div>
    <div class="maskBg" style="display:none">
        <div class="imgPop"><img id = "previewImg" src=""/></div>
        <div class="bomDiv">
            <a href="javascript:;" class="jb" onclick="reportImg()" title="举报"></a>
            <a href="javascript:;" class="close" title="关闭"></a>
        </div>
    </div>

    <script>
        jQuery(function(){
            jQuery(".clickme").click(function(){
                var srcStr = jQuery(this).attr("src");
                jQuery(".maskBg").find("#previewImg").attr("src", srcStr);
                jQuery(".maskBg").show();
            });

            jQuery(".ans-cc img").click(function(){
                var srcStr = jQuery(this).attr("src");
                jQuery(".maskBg").find("#previewImg").attr("src", srcStr);
                jQuery(".maskBg").show();
            });

            jQuery(".close").click(function(){
                jQuery(".maskBg").hide();
            });
        })

        function reportImg() {
            var imgSrc = jQuery(".maskBg").find("#previewImg").attr("src");
            var objectId = imgSrc.substring(imgSrc.lastIndexOf("/")+1, imgSrc.lastIndexOf("."));
            var params = new Array();

            params.push({'name':'type', 'value':'image'});
            params.push({'name':'sourceIdstr', 'value': jQuery.md5(objectId)});
            params.push({'name':'sourceContent', 'value': encodeURIComponent(JSON.stringify({'image':imgSrc}))});
            FORM_UTILS.post('https://groupweb.chaoxing.com/pc/report/reportIndex', params, '_blank');
        }
    </script>
</div>
